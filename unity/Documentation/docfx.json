{
    "metadata": [
        {
            "src": [
                {
                    "src": "../Assets/Scripts",
                    "files": ["**/*.cs"]
                }
            ],
            "dest": "api",
            "filter": "filterConfig.yml",
            "allowCompilationErrors": true
        }
    ],
    "build": {
        "content": [
            {
                "files": ["**/*.{md,yml}"],
                "exclude": ["_site/**"]
            }
        ],
        "output": "./clientHTMLOutput",
        "template": ["default", "modern"],
        "globalMetadata": {
            "_appName": "ARFlow",
            "_appTitle": "ARFlow",
            "_enableSearch": true,
            "pdf": true
        }
    }
}
